{
  "authentication": {
    "url": "https://github.com/login",
    "form": {
      "fields": {
        "#login_field": "username",
        "#password": "password"
      },
      "submit": "Sign in"
    }
  },
  "targets": [
    {
      "url": "https://github.com/notifications",
      "requests": 5
    },
    {
      "url": "https://github.com/settings/profile",
      "requests": 5
    }
  ]
}
